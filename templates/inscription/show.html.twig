{% extends 'base.html.twig' %}

{% block title %}Inscription emission{% endblock %}

{% block body %}
    <h1>Emission {{ inscription.EMS}}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>DATE</th>
                <td>{{ inscription.DATE ? inscription.DATE|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Emission</th>
                <td>{{ inscription.EMS }}</td>
            </tr>
        <tbody>
    
        {% for participant in participants %}
            <tr>
                <td>{{ participant.PRENOM }}</td>
                <td>{{ participant.NOM }}</td>
                <td>{{ participant.TEL }}</td>
                <td>{{ participant.MAIL }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
      
        </tbody>
    </table>


    

    <a href="{{ path('app_creation') }}">back to list</a>

    <a href="{{ path('app_inscription_edit', {'id': inscription.id}) }}">edit</a>

    {{ include('inscription/_delete_form.html.twig') }}
{% endblock %}
