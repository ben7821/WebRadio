{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inscription.id }}</td>
            </tr>
            <tr>
                <th>NOM</th>
                <td>{{ inscription.NOM }}</td>
            </tr>
            <tr>
                <th>PRENOM</th>
                <td>{{ inscription.PRENOM }}</td>
            </tr>
            <tr>
                <th>TEL</th>
                <td>{{ inscription.TEL }}</td>
            </tr>
            <tr>
                <th>MAIL</th>
                <td>{{ inscription.MAIL }}</td>
            </tr>
            <tr>
                <th>DATE</th>
                <td>{{ inscription.DATE ? inscription.DATE|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_inscription_index') }}">back to list</a>

    <a href="{{ path('app_inscription_edit', {'id': inscription.id}) }}">edit</a>

    {{ include('inscription/_delete_form.html.twig') }}
{% endblock %}
